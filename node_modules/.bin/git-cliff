#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/n1co/ws/n1co/cliff-test/node_modules/.pnpm/git-cliff@1.2.0/node_modules/git-cliff/lib/node_modules:/Users/n1co/ws/n1co/cliff-test/node_modules/.pnpm/git-cliff@1.2.0/node_modules/git-cliff/node_modules:/Users/n1co/ws/n1co/cliff-test/node_modules/.pnpm/git-cliff@1.2.0/node_modules:/Users/n1co/ws/n1co/cliff-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/n1co/ws/n1co/cliff-test/node_modules/.pnpm/git-cliff@1.2.0/node_modules/git-cliff/lib/node_modules:/Users/n1co/ws/n1co/cliff-test/node_modules/.pnpm/git-cliff@1.2.0/node_modules/git-cliff/node_modules:/Users/n1co/ws/n1co/cliff-test/node_modules/.pnpm/git-cliff@1.2.0/node_modules:/Users/n1co/ws/n1co/cliff-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../git-cliff/lib/index.js" "$@"
else
  exec node  "$basedir/../git-cliff/lib/index.js" "$@"
fi
